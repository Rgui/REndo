\name{Data_hmlewbel}
\alias{Data_hmlewbel}
\docType{data}
\title{
Simulated dataset with one endogenous regressor
}
\description{
A simulated dataset to exemplify the use of the \code{\link{hmlewbel}} function. 
}
\usage{data("Data_hmlewbel")}
\format{
  A data frame with 2500 observations on the following 4 variables.
  \describe{
    \item{\code{y1}}{a numeric vector representing the dependent variable.}
    \item{\code{X1}}{a numeric vector representing an exogenous regressor.}
    \item{\code{X2}}{a numeric vector representing an exogenous regressor.}
    \item{\code{P1}}{a numeric vector representing an endogenous regressor.}
  }
}
\keyword{datasets}
